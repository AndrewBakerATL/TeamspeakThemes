/*------------------------------------------------------------------------------------------------
									QToolBar - Top Menubar
--------------------------------------------------------------------------------------------------*/	
 
QToolBar	{
	background-image: url('styles/StarCitizen/Borders/line-bg.png');
	background-color: #001019;
	background-position: left;
	background-repeat: repeat-xy;
	background-attachment:inherit;
	margin: 0px 2px 0px 2px;
	border: transparent;
	spacing: 2px;
	color: #6ABBCF;
}

/*Not working? Tried to paint it*/
QToolBarSeparator::QToolBarSeparator { }

/* All buttons on Toolbar */
QToolBar QToolButton {
	border: transparent;
	background-color: none;
} 

/* Default state of toolbutton with dropdown menu (Such as the Away button or Mute Speakers */
QToolBar QToolButton[popupMode="1"] { 
	border: 1px solid transparent;
	border-radius: none;
	background-color: transparent;
	padding-right: 10px;
	color: #6ABBCF; /* Use color to change the color of the arrow for the pop-up menu */
}

QToolBar QToolButton:hover[popupMode="1"] { 
	border: 1px solid transparent;
	border-radius: none;
	background-color: transparent;
	padding-right: 10px;
	color: #00CCFF; /* Use color to change the color of the hover arrow for the pop-up menu */
}

QToolBar QToolButton:disabled {
	background-attachment:inherit;
	border: 5px solid transparent;
	border-image: none;
}

QToolBar QToolButton::menu-button {	border: 1px solid transparent; width: 12px;  } 
QToolBar QToolButton::menu-button:hover { }
QToolBar QToolButton::menu-arrow { left: 0px; } /* Change the dropdown menu button on a toolbutton using image */
QToolBar QToolButton::menu-arrow:open { left: 0px; } /* Sets the open state of the menu arrow */

/*  Use QToolBar::handle for generic horizontal/vertical handle*/
QToolBar::handle:vertical,
QToolBar::handle:vertical:selected:active {
	background-color: transparent; 
	background-image: url('styles/StarCitizen/TreeView/pointeroff.png');
	background-attachment: inherit;
	background-repeat: repeat-x;
	width: 20px;
	height: 10px;
	padding-top: 4px;
	padding-bottom: 4px;
}

QToolBar::handle:vertical:hover {
	background-color: transparent; 
	background-image: url('styles/StarCitizen/TreeView/pointer.png');
	background-attachment: inherit;
	background-repeat: repeat-x;
	width: 20px;
	height: 10px;
	padding-top: 4px;
	padding-bottom: 4px;
}
 
QToolBar::handle:horizontal,
QToolBar::handle:horizontal:selected:active {
	background-color: transparent;  
	background-image: url('styles/StarCitizen/ChatArea/toolgriph.png');
	background-attachment: inherit;
	background-repeat: repeat-y;
	width: 10px;
	height: 20px;
	padding-right: 4px;
	margin-left: 4px;
}

QToolBar::handle:horizontal:hover {
	background-color: transparent; 
	background-image: url('styles/StarCitizen/ChatArea/toolgriphh.png');
	background-attachment: inherit;
	background-repeat: repeat-y;
	width: 10px;
	height: 20px;
	padding-right: 4px;
	margin-left: 4px;
}
 
